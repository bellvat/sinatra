<html>
<head>
  <meta charset="UTF-8">
  <script type="text/javascript" src="hangman.js" defer></script>
  <link rel="stylesheet" href="hangman.css" />
</head>
<body>
  <input id = "word_input" value = "<%= length %>" />
  <input id = 'arr' name= '<%= alphabet %>' value = '<%= place %>'/>
  <input id = 'guess' value='<%= @@hangman_guess %>' />
  <div class='left'>
    <div class='play' >
    </div>
    <div class='letters'>
    </div>
    <% if @@hangman_guess == 9 %>
      <%= @@word %>
    <% end %>
  </div>
  <div class='stick'>
  </div>
</body>
</html
